{
  "schema_version": 1,
  "quilt_loader": {
    "group": "org.quiltmc",
    "id": "quilt_loader",
    "version": "${version}",
    "metadata": {
      "name": "Quilt Loader",
      "description": "The mod-loader that cares.",
      "contributors": {
        "QuiltMC: Loader Team": "Owner"
      },
      "contact": {
        "homepage": "https://quiltmc.org",
        "issues": "https://github.com/QuiltMC/quilt-loader/issues",
        "sources": "https://github.com/QuiltMC/quilt-loader"
      },
      "license": "Apache-2.0",
      "icon": "assets/quilt_loader/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "provides": [
        {
          "id": "fabricloader",
          "version": "0.15.11"
        }
      ],
      "depends": [
        "mixinextras"
      ]
  }
}
